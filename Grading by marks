sub1=int(input())
sub2=int(input())
sub3=int(input())
sub4=int(input())
sub5=int(input())
total=sub1+sub2+sub3+sub4+sub5
print("Total marks:",total)
avg=total/5
print("Avg marks:",avg)
if(avg>=90):
    print("Grade A")
elif(avg>=80) and (avg<89):
    print("Grade B")
elif(avg>=70) and (avg<79):
    print("Grade C")
elif(avg>=60) and (avg<69):
    print("Grade D")
else:
    print("Fail")
